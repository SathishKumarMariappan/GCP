{
    "steps": [
        {
            "name": "node:20",
            "entrypoint": "bash",
            "args": ["-c", "cd gcp-ng && npm install"]
        },
        {
            "name": "Build",
            "entrypoint": "bash",
            "args": ["-c", "cd gcp-ng && npm run build"]
        },
        {
            "name": "Publish",
            "args": ["cp", "-r", "gcp-ng/dist/gcp-ng/browser/*", "gs://gcp-cloud-build-20240730"]
        }
    ],
    "timeout": "1200s",
    "logsBucket": "gs://gcp-cloud-build-20240730-logs"
}
